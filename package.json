{
    "name": "FinTechToken",
    "version": "1.0.0",
    "description": "FinTechToken Website",
    "scripts": {
      "build": "tsc",
      "build:watch": "tsc -w",
      "build:e2e": "tsc -p e2e/",
      "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
      "serve": "lite-server",
      "serve:e2e": "lite-server -c=bs-config.e2e.json",
      "serve:aot": "cp indexAOT.html index.html && lite-server -c=tsconfig-aot.json",
      "prestart": "npm run build",
      "start": "cp indexJIT.html index.html && concurrently \"npm run build:watch\" \"npm run serve\"",
      "pree2e": "npm run build:e2e",
      "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
      "preprotractor": "webdriver-manager update",
      "protractor": "protractor protractor.config.js",
      "pretest": "npm run build",
      "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
      "pretest:once": "npm run build",
      "test:once": "karma start karma.conf.js --single-run",
      "lint": "tslint ./**/*.ts -t verbose"
    },
    "keywords": [],
    "author": "FinTechToken",
    "license": "",
    "dependencies": {
      "@angular/common": "~4.4.6",
      "@angular/compiler": "~4.4.6",
      "@angular/core": "~4.4.6",
      "@angular/forms": "~4.4.6",
      "@angular/http": "~4.4.6",      
      "@angular/platform-browser": "~4.4.6",
      "@angular/platform-browser-dynamic": "~4.4.6",
      "@angular/router": "~4.4.6",
      "angular-in-memory-web-api": "~0.5.1",
      "systemjs": "0.20.19",
      "core-js": "2.5.1",
      "rxjs": "5.4.2",
      "zone.js": "0.8.18",

      "@angular/compiler-cli": "~4.4.6",      
      "@angular/platform-server": "~4.4.6",

      "@angular/animations": "~4.4.6"
    },
    "devDependencies": {
      "concurrently": "~3.5.0",
      "lite-server": "~2.3.0",
      "typescript": "~2.5.3",
      "canonical-path": "0.0.2",
      "tslint": "~5.8.0",
      "lodash": "~4.17.4",
      "jasmine-core": "~2.8.0",
      "karma": "~1.7.1",
      "karma-chrome-launcher": "~2.2.0",
      "karma-cli": "~1.0.1",
      "karma-jasmine": "~1.1.0",
      "karma-jasmine-html-reporter": "~0.2.2",
      "protractor": "~5.2.0",
      "rimraf": "~2.6.2",
      "@types/node": "8.0.46",
      "@types/jasmine": "2.6.0",

      "rollup": "~0.50.0",
      "rollup-plugin-commonjs": "~8.2.4",
      "rollup-plugin-node-resolve": "~3.0.0",
      "rollup-plugin-uglify": "~2.0.1"
    },
    "repository": {}
  }
  