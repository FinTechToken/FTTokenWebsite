{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [ "es2015", "dom" ],
      "noImplicitAny": false,
      "suppressImplicitAnyIndexErrors": true,
      "outDir": "compiled"
    },
    "exclude": [
      "node_modules",
      "aot/*",
      "e2e/*",
      "app/main.ts",
      "compiled"
    ]
  }
  